{
  "projectType": "library",
  "root": "libs/asterisk-common",
  "sourceRoot": "libs/asterisk-common/src",
  "prefix": "asterisk-frontend",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/asterisk-common"],
      "options": {
        "jestConfig": "libs/asterisk-common/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/asterisk-common/src/**/*.ts",
          "libs/asterisk-common/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
